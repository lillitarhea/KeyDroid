<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.android.keydroid.samrating">

    <ImageButton
        android:layout_width="110dp"
        android:layout_height="100dp"
        android:id="@+id/pleasure_1"
        android:src="@drawable/pleasure_1"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:onClick="pleasure1"/>

    <ImageButton
        android:layout_width="110dp"
        android:layout_height="100dp"
        android:id="@+id/pleasure_2"
        android:src="@drawable/pleasure_2"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@+id/pleasure_1"
        android:layout_toEndOf="@+id/pleasure_1" />


    <ImageButton
        android:layout_width="110dp"
        android:layout_height="100dp"
        android:id="@+id/pleasure_3"
        android:src="@drawable/pleasure_3"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@+id/pleasure_2"
        android:layout_toEndOf="@+id/pleasure_2" />

    <ImageButton
        android:layout_width="110dp"
        android:layout_height="100dp"
        android:id="@+id/pleasure_4"
        android:src="@drawable/pleasure_4"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@+id/pleasure_3"
        android:layout_toEndOf="@+id/pleasure_3" />

    <ImageButton
        android:layout_width="110dp"
        android:layout_height="100dp"
        android:id="@+id/pleasure_5"
        android:src="@drawable/pleasure_5"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@+id/pleasure_4"
        android:layout_toEndOf="@+id/pleasure_4" />

    <ImageButton
        android:layout_width="110dp"
        android:layout_height="100dp"
        android:id="@+id/arousal_1"
        android:src="@drawable/arousal_1"
        android:layout_below="@+id/pleasure_1"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="32dp"
        android:onClick="arousal1"/>

    <ImageButton
        android:layout_width="110dp"
        android:layout_height="100dp"
        android:id="@+id/arousal_2"
        android:src="@drawable/arousal_2"
        android:layout_alignTop="@+id/arousal_1"
        android:layout_alignLeft="@+id/pleasure_2"
        android:layout_alignStart="@+id/pleasure_2" />

    <ImageButton
        android:layout_width="110dp"
        android:layout_height="100dp"
        android:id="@+id/arousal_3"
        android:src="@drawable/arousal_3"
        android:layout_alignTop="@+id/arousal_2"
        android:layout_toRightOf="@+id/pleasure_2"
        android:layout_toEndOf="@+id/pleasure_2" />

    <ImageButton
        android:layout_width="110dp"
        android:layout_height="100dp"
        android:id="@+id/arousal_4"
        android:src="@drawable/arousal_4"
        android:layout_alignTop="@+id/arousal_3"
        android:layout_alignLeft="@+id/pleasure_4"
        android:layout_alignStart="@+id/pleasure_4" />

    <ImageButton
        android:layout_width="110dp"
        android:layout_height="100dp"
        android:id="@+id/arousal_5"
        android:src="@drawable/arousal_5"
        android:layout_alignTop="@+id/arousal_4"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_alignBottom="@+id/arousal_4" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="OK"
        android:onClick="Next"
        android:id="@+id/ok"
        android:layout_below="@+id/arousal_3"
        android:layout_toLeftOf="@+id/arousal_4"
        android:layout_toStartOf="@+id/arousal_4" />


</RelativeLayout>
